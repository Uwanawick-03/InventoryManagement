	<parameter name="Order_Id" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    o.Order_Id,
    o.Order_Date,
    c.Cust_Name,
    c.Phone,
    c.Email,
    c.Branch_Name,
    i.Item_Name,
    oi.Qty,
    i.Price,
    oi.Total_Price,
    o.Total_Price AS Invoice_Total
FROM 
    orders o
JOIN 
    customer c ON o.Cust_Id = c.Cust_Id
JOIN 
    order_items oi ON o.Order_Id = oi.Order_Id
JOIN 
    item i ON oi.Item_Id = i.Item_Id
WHERE 
   WHERE o.Order_Id = $P{Order_Id}
ORDER BY 
    o.Order_Id, i.Item_Name;]]>
	</queryString>
